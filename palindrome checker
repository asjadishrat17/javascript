function palindrome(str) {
  var str1 = str.toUpperCase();
      str1 = str1.match(/[a-zA-Z0-9]/gi);
      str=str.toUpperCase();
   str=str.match(/[a-zA-Z0-9]/gi);
   str1=str1.reverse();
 
 console.log(str);
 console.log(str1);
 if(JSON.stringify(str)==JSON.stringify(str1)){
   return true;
 }
 else 
 return false;
}
palindrome("My age is 0, 0 si ega ym.");
